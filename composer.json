{
  "name": "nextevent/php-sdk",
  "description": "SDK to interact with the NextEvent API written in PHP",
  "homepage": "https://nextevent.com",
  "keywords": [
    "NextEvent",
    "PHP",
    "SDK"
  ],
  "license": "MIT",
  "support": {
    "source": "https://github.com/nextevent/php-sdk",
    "docs": "https://docs.nextevent.com"
  },
  "require": {
    "php": ">=5.4",
    "guzzlehttp/guzzle": "~5.3",
    "psr/log": "1.0.2"
  },
  "autoload": {
    "psr-4": {
      "NextEvent\\PHPSDK\\": "src"
    }
  },
  "repositories": {
    "nexbyte-codestyle": {
      "type": "vcs",
      "url": "ssh://git@git.nexbyte.ch/diffusion/NCS/nexbyte-codestyle.git"
    }
  },
  "require-dev": {
    "nexbyte/codestyle": "dev-master",
    "phpunit/phpunit": "~4.8.20",
    "phpdocumentor/phpdocumentor": "2.*"
  }
}
